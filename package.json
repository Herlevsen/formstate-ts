{
  "name": "formstate-ts",
  "version": "0.6.0",
  "description": "type safe typescript library for managing form state in react",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Herlevsen/formstate-ts"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "pretest": "npm run lint",
    "test": "jest",
    "build": "rimraf lib && tsc && npm run docs",
    "docs": "typedoc --out docs ./src",
    "lint": "eslint --ext ts --ext tsx src/*",
    "precommit": "npm run prettier && npm run lint",
    "prettier": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\" \"test/**/*.{ts,tsx,js,jsx,json}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Jens Herlevsen",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-docs": "^5.2.5",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.16.0",
    "jest": "^24.9.0",
    "prettier": "1.18.2",
    "react": "^16.11.0",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-dom": "^16.10.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3",
    "utility-types": "^3.8.0"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  }
}
