{
  "name": "formstate-ts",
  "version": "0.5.0",
  "description": "type safe typescript library for managing form state in react",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/Herlevsen/formstate-ts"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "jest",
    "build": "rimraf lib && tsc && npm run docs",
